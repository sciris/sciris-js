{
  "name": "sciris-js",
  "version": "0.2.11",
  "license": "MIT",
  "author": "Sciris <info@sciris.org>",
  "repository": {
    "type": "git",
    "url": "http://github.com/sciris/sciris-js"
  },
  "scripts": {
    "build": "bili && npm run export",
    "export": "npx browserify main.js --standalone sciris -o dist/sciris-js.js",
    "test": "npm run build && ./node_modules/.bin/mocha tests --require @babel/register --require jsdom-global/register",
    "build_docs": "node_modules/jsdoc/jsdoc.js src/ -r -c .jsdocs -d docs/ -t node_modules/docdash/" 
  },
  "main": "dist/sciris-js.cjs.js",
  "module": "dist/sciris-js.es.js",
  "files": [
    "dist/*"
  ],
  "bili": {
    "css": true,
    "exports": "named",
    "plugins": [
      "vue"
    ],
    "vue": {
      "css": true
    },
    "postcss": {
      "extract": false
    },
    "format": [
      "cjs",
      "es",
      "umd"
    ],
    "banner": true,
    "name": "sciris-js",
    "moduleName": "sciris-js"
  },
  "bundleDependencies": false,
  "dependencies": {
    "@babel/runtime": "^7.4.2",
    "acorn": "^6.0.2",
    "axios": "^0.18.0",
    "cryptiles": "^4.1.2",
    "crypto": "^1.0.1",
    "crypto-api": "^0.8.0",
    "crypto-js": "^3.1.9-1",
    "epic-spinners": "^1.0.4",
    "file-saver": "^2.0.0-rc.4",
    "js-sha256": "^0.9.0",
    "mpld3": "^0.4.1",
    "vue": "^2.5.17",
    "vue-clickaway": "^2.2.2",
    "vue-dialog-drag": "^0.1.29",
    "vue-js-modal": "^1.3.26",
    "vue-progressbar": "git+https://github.com/sciris/vue-progressbar.git"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/plugin-transform-runtime": "^7.4.0",
    "@babel/polyfill": "^7.4.0",
    "@babel/preset-env": "^7.4.1",
    "@babel/register": "^7.4.0",
    "assert": "^1.4.1",
    "babel-loader": "^8.0.5",
    "bili": "^3.4.2",
    "browserify": "^16.2.3",
    "chai": "^4.2.0",
    "docdash": "^1.0.3",
    "esmify": "^2.0.0",
    "jsdoc": "^3.5.5",
    "jsdoc-strip-async-await": "^0.1.0",
    "jsdom": "^14.0.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^6.0.2",
    "nock": "^10.0.6",
    "node-sass": "^4.10.0",
    "parse5": "^2.2.3",
    "rollup": "^0.47.6",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-postcss": "^1.6.2",
    "rollup-plugin-vue": "^4.3.2",
    "vue-template-compiler": "^2.5.17"
  }
}
