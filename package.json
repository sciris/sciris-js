{
  "name": "sciris-js",
  "version": "0.1.0",
  "author": "Optima Consortium <info@ocds.co>",
  "scripts": {
    "build": "bili"
  },
  "main": "dist/sciris-js.cjs.js",
  "module": "dist/sciris-js.es.js",
  "files": [
    "dist/*",
    "src/*",
    "*.js",
    "*.json"
  ],
  "babel": {
    "presets": [
      [
        "vue-app"
      ]
    ]
  },
  "bili": {
    "css": true,
    "exports": "named",
    "plugins": [
      "vue"
    ],
    "vue": {
      "css": true
    },
    "postcss": {
      "extract": false 
    },
    "format": [
      "cjs",
      "es",
      "umd",
      "umd-min"
    ],
    "banner": true,
    "name": "sciris-js",
    "moduleName": "sciris-js"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "cryptiles": "^4.1.2",
    "crypto": "^1.0.1",
    "crypto-api": "^0.8.0",
    "crypto-js": "^3.1.9-1",
    "file-saver": "^2.0.0-rc.4",
    "js-sha256": "^0.9.0",
    "vue": "^2.5.17",
    "vue-dialog-drag": "^0.1.29",
    "vue-js-modal": "^1.3.26",
    "vue-progressbar": "git@github.com:sciris/vue-progressbar.git",
    "acorn": "^6.0.2",
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-vue-app": "^2.0.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-component": "^1.1.1",
    "babel-plugin-preval": "^3.0.1"
  },
  "devDependencies": {
    "bili": "^3.1.2",
    "parse5": "^2.2.3",
    "rollup": "^0.47.6",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-postcss": "^1.6.2",
    "rollup-plugin-vue": "^2.5.4"
  }
}
